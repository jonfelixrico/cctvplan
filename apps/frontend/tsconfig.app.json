{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "out-tsc/frontend",
    "rootDir": "src",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "tsBuildInfoFile": "out-tsc/frontend/tsconfig.app.tsbuildinfo"
  },
  "include": [
    ".nuxt/nuxt.d.ts",
    "src/**/*"
  ],
  "exclude": [
    "out-tsc",
    "dist",
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ]
}
